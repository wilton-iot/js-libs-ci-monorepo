{
    "appname": "kiosk",
    "appdir": "{{{appdir}}}",

    "server": {
        "ipAddress": "127.0.0.1",
        "tcpPort": 8080,
        "cacheMaxAgeSeconds": 0,
        "stdlibFileName": "std.wlib"
    },

    "client": {
        "url": "http://127.0.0.1:8080",
        "fullscreen": false,
        "windowWidth": 800,
        "windowHeight": 600
    },

    "logging": {
        "appenders": [{
            "appenderType": "CONSOLE",
            "thresholdLevel": "DEBUG"
        }],
        "loggers": {
            "staticlib": "WARN",
            "wilton": "INFO",
            "kiosk": "DEBUG"
        }
    }
}
