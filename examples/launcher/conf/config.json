{
    "appname": "launcher",
    "appdir": "{{{appdir}}}",

    "server": {
        "ipAddress": "127.0.0.1",
        "tcpPort": 26100,
        "cacheMaxAgeSeconds": 0,
        "stdlibFileName": "std.wlib"
    },

    "client": {
        "url": "http://127.0.0.1:26100",
        "fullscreen": false,
        "windowWidth": 800,
        "windowHeight": 600
    },

    "web": {
        "wsUrl": "ws://127.0.0.1:26100/launcher/server/views/websocket",
        "wsTimeoutMillis": 30000,
        "appPort": 8080
    },

    "git": {
        "sshPrivateKeyPath": "/sdcard/Android/data/wilton.android/files/ssh/id_rsa",
        "sshPublicKeyPath": "/sdcard/Android/data/wilton.android/files/ssh/id_rsa.pub",
        "branch": "master"
    },

    "logging": {
        "appenders": [
            {
                "appenderType": "DAILY_ROLLING_FILE",
                "thresholdLevel": "DEBUG",
                "filePath": "{{{appdir}}}/log/launcher_log.txt"
            },
            {
            "appenderType": "CONSOLE",
            "thresholdLevel": "DEBUG"
            }
        ],
        "loggers": {
            "staticlib": "WARN",
            "wilton": "INFO",
            "launcher": "DEBUG"
        }
    }
}
