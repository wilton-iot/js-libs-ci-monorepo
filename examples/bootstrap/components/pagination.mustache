<ul class="pagination">
    {{#paginationButtons}}
        {{#normal}}
        <li class="page-item"><a class="page-link text-secondary" href="{{url}}{{index}}">{{label}}</a></li>
        {{/normal}}
        {{#active}}
        <li class="page-item"><a class="page-link bg-secondary text-white" href="#">{{label}}</a></li>
        {{/active}}
        {{#ellipsis}}
            <li class="page-item disabled"><a class="page-link" href="#">{{label}}</a></li>
        {{/ellipsis}}
    {{/paginationButtons}}
</ul>
